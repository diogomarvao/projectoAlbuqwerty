package com.musicstadium.model;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Table;


public class Event extends Entity{

//	Variaveis
	private String name;
	
//	@Column(name="Date (Start)")
//	private Date dateS;
//	
//	@Column(name="Date (End)")
//	private Date dateF;
	
	private DateTime dateS = new DateTime();
	private DateTime dateF = new DateTime();
	
	
	private String description;
	
	private String localization;
	
	private String genre;
	
	
	
	//getter & setters
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public DateTime getDateS() {
		return dateS;
	}
	public void setDateS(DateTime dateS) {
		this.dateS = dateS;
	}
	public DateTime getDateF() {
		return dateF;
	}
	public void setDateF(DateTime dateF) {
		this.dateF = dateF;
	}
	public String getLocalization() {
		return localization;
	}
	public void setLocalization(String localization) {
		this.localization = localization;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public String getGenre() {
		return genre;
	}
	public void setGenre(String genre) {
		this.genre = genre;
	}
	
	
//	Construtor Event
	public Event(){
		
	}
	
	
}
